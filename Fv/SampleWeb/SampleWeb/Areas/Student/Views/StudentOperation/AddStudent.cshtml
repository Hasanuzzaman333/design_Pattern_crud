@*@model SampleData.Models.StudentModel*@
@{
    ViewData["Title"] = "AddStudent";
    Layout = "~/Views/Shared/_customLayout.cshtml";
}

    <form role="form" action="/Student/StudentOperation/AddStudent" method="post">
        <div class="form-group">
            <label for="exampleInputEmail1">Student Name</label>
            <input type="text" class="form-control" id="" name="StudentName" placeholder="Enter Name">
            @*<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>*@
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Student Age</label>
            <input type="text" class="form-control" id="" name="StudentAge" placeholder="Enter Age">
            @*<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>*@
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>

